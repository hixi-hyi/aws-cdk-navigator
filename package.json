  },
  "author": "Hiroyoshi HOUCHI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/hixi-hyi/aws-cdk-navigator.git"
  },
  "bugs": {
    "url": "https://github.com/hixi-hyi/aws-cdk-navigator/issues"
  },
  "homepage": "https://github.com/hixi-hyi/aws-cdk-navigator#readme",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "aws-cdk-lib": "^2.121.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.2",
    "ts-jest": "^29.1.1",
    "tsc": "^2.0.4",
    "tslint": "^6.1.3",
    "typescript": "^5.3.3"
